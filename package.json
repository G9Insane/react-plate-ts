{
  "name": "react-plate-ts",
  "version": "0.0.1",
  "license": "MIT",
  "author": "G9Insane",
  "description": "React Template Type Script",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:script": "eslint --ext .ts,tsx --ignore-path .gitignore ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.4.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/parser": "^4.28.5",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "peerDependencies": {
    "prettier": "^2.3.2"
  }
}
